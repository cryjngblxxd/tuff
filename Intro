local ArcsContainer = Instance.new("Frame")
ArcsContainer.Parent = CenterContainer
ArcsContainer.BackgroundTransparency = 1
ArcsContainer.Size = UDim2.new(1, 0, 1, 0)
ArcsContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
ArcsContainer.AnchorPoint = Vector2.new(0.5, 0.5)
ArcsContainer.ZIndex = 15

local Arcs = {}
for i = 1, 3 do
	local arcSize = 280 + (i * 40)
	
	local arc = Instance.new("Frame")
	arc.Parent = ArcsContainer
	arc.BackgroundTransparency = 1
	arc.Size = UDim2.new(0, arcSize, 0, arcSize)
	arc.Position = UDim2.new(0.5, 0, 0.5, 0)
	arc.AnchorPoint = Vector2.new(0.5, 0.5)
	arc.ZIndex = 15
	
	local arcCorner = Instance.new("UICorner")
	arcCorner.Parent = arc
	arcCorner.CornerRadius = UDim.new(1, 0)
	
	local arcStroke = Instance.new("UIStroke")
	arcStroke.Parent = arc
	arcStroke.Color = i == 1 and Config.AccentBlue or i == 2 and Config.LightBlue or Config.PrimaryBlue
	arcStroke.Thickness = 2
	arcStroke.Transparency = 1
	
	local arcGradient = Instance.new("UIGradient")
	arcGradient.Parent = arcStroke
	arcGradient.Rotation = 0
	arcGradient.Transparency = NumberSequence.new({
		NumberSequenceKeypoint.new(0, 0),
		NumberSequenceKeypoint.new(0.15, 0.5),
		NumberSequenceKeypoint.new(0.3, 1),
		NumberSequenceKeypoint.new(0.7, 1),
		NumberSequenceKeypoint.new(0.85, 0.5),
		NumberSequenceKeypoint.new(1, 0),
	})
	
	table.insert(Arcs, {
		Frame = arc,
		Stroke = arcStroke,
		Gradient = arcGradient,
		Speed = (4 - i) * 25,
		Direction = i % 2 == 0 and 1 or -1,
	})
end


