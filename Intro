local SlashContainer = Instance.new("Frame")
SlashContainer.Parent = ScreenGui
SlashContainer.BackgroundTransparency = 1
SlashContainer.Size = UDim2.new(1, 0, 1, 0)
SlashContainer.ZIndex = 5

local function CreateSlash(angle, delay, color, thickness, speed)
	spawn(function()
		wait(delay)
		
		local slash = Instance.new("Frame")
		slash.Parent = SlashContainer
		slash.BackgroundColor3 = color or Config.AccentBlue
		slash.BackgroundTransparency = 1
		slash.Size = UDim2.new(0, 0, 0, thickness or 3)
		slash.Position = UDim2.new(Config.LogoX, 0, Config.LogoY, 0)
		slash.AnchorPoint = Vector2.new(0.5, 0.5)
		slash.Rotation = angle
		slash.BorderSizePixel = 0
		slash.ZIndex = 5
		
		local slashGradient = Instance.new("UIGradient")
		slashGradient.Parent = slash
		slashGradient.Transparency = NumberSequence.new({
			NumberSequenceKeypoint.new(0, 1),
			NumberSequenceKeypoint.new(0.3, 0),
			NumberSequenceKeypoint.new(0.7, 0),
			NumberSequenceKeypoint.new(1, 1),
		})
		
		local slashGlow = Instance.new("Frame")
		slashGlow.Parent = slash
		slashGlow.BackgroundColor3 = color or Config.AccentBlue
		slashGlow.BackgroundTransparency = 0.5
		slashGlow.Size = UDim2.new(1, 0, 5, 0)
		slashGlow.Position = UDim2.new(0, 0, 0.5, 0)
		slashGlow.AnchorPoint = Vector2.new(0, 0.5)
		slashGlow.BorderSizePixel = 0
		slashGlow.ZIndex = 4
		
		local glowGradient = Instance.new("UIGradient")
		glowGradient.Parent = slashGlow
		glowGradient.Transparency = NumberSequence.new({
			NumberSequenceKeypoint.new(0, 1),
			NumberSequenceKeypoint.new(0.3, 0.6),
			NumberSequenceKeypoint.new(0.7, 0.6),
			NumberSequenceKeypoint.new(1, 1),
		})
		
		Tween(slash, 0.08, { BackgroundTransparency = 0 })
		Tween(slash, speed or 0.25, { Size = UDim2.new(3, 0, 0, thickness or 3) }, Enum.EasingStyle.Quint)
		
		wait(speed or 0.25)
		Tween(slash, 0.15, { BackgroundTransparency = 1 })
		Tween(slashGlow, 0.15, { BackgroundTransparency = 1 })
		
		wait(0.2)
		slash:Destroy()
	end)
end

local function CreateSlashBurst()
	local slashAngles = {-45, 45, -30, 30, -60, 60, 0, 90, -15, 15, -75, 75}
	local colors = {Config.AccentBlue, Config.LightBlue, Config.ElectricBlue, Config.White}
	
	for i, angle in ipairs(slashAngles) do
		local color = colors[math.random(1, #colors)]
		local thickness = math.random(2, 4)
		local speed = math.random(20, 35) / 100
		CreateSlash(angle, (i - 1) * 0.03, color, thickness, speed)
	end
end

local function CreateRandomSlash()
	local angle = math.random(-80, 80)
	local colors = {Config.AccentBlue, Config.LightBlue, Config.ElectricBlue, Config.White}
	local color = colors[math.random(1, #colors)]
	local thickness = math.random(1, 3)
	local speed = math.random(15, 30) / 100
	CreateSlash(angle, 0, color, thickness, speed)
end


