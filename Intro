LoadingPercent.ZIndex = 30


local ElapsedTime = 0
local Connection
local BaseLogoSize = 160
local LoadingProgress = 0
local SlashesActive = false

local function StartAnimation()
	Tween(Background, 0.6, { BackgroundTransparency = 0 })
	Tween(Vignette, 0.8, { ImageTransparency = 0.1 })
	
	Tween(Blur, 1.5, { Size = 18 })
	Tween(ColorCorrection, 1, {
		Brightness = -0.08,
		Contrast = 0.2,
		Saturation = 0.15,
	})
	Tween(Bloom, 1, { Intensity = 0.5 })
	
	wait(0.3)
	
	CreateSlashBurst()
	
	Tween(ColorCorrection, 0.08, { Brightness = 0.3 })
	wait(0.08)
	Tween(ColorCorrection, 0.2, { Brightness = -0.08 })
	
	wait(0.3)
	
	CreateSlashBurst()
	
	wait(0.2)
	
	Tween(OuterRingGlow, 0.6, { Size = UDim2.new(0, 320, 0, 320) }, Enum.EasingStyle.Back)
	Tween(OuterRingGlowStroke, 0.5, { Transparency = 0.7 })
	
	CreateSlash(45, 0, Config.AccentBlue, 3, 0.2)
	CreateSlash(-45, 0.05, Config.LightBlue, 2, 0.2)
	
	wait(0.08)
	
	Tween(OuterRing, 0.5, { Size = UDim2.new(0, 280, 0, 280) }, Enum.EasingStyle.Back)
	Tween(OuterRingStroke, 0.4, { Transparency = 0.2 })
	
	wait(0.08)
	
	Tween(MiddleRing, 0.5, { Size = UDim2.new(0, 230, 0, 230) }, Enum.EasingStyle.Back)
	Tween(MiddleRingStroke, 0.4, { Transparency = 0.3 })
	
	CreateSlash(30, 0, Config.White, 2, 0.25)
	CreateSlash(-60, 0.03, Config.ElectricBlue, 2, 0.25)
	
	wait(0.08)
	
	Tween(InnerRing, 0.5, { Size = UDim2.new(0, 180, 0, 180) }, Enum.EasingStyle.Back)
	Tween(InnerRingStroke, 0.4, { Transparency = 0.4 })
	
	for i, arc in ipairs(Arcs) do
		spawn(function()
			wait(i * 0.1)
			Tween(arc.Stroke, 0.4, { Transparency = 0.2 })
		end)
	end
	
	wait(0.3)
	
	CreateSlash(0, 0, Config.AccentBlue, 4, 0.3)
	CreateSlash(90, 0.1, Config.LightBlue, 3, 0.3)
	
	wait(0.2)
	
	SlashesActive = true
	spawn(function()
		while SlashesActive do
			CreateRandomSlash()
			wait(math.random(10, 18) / 100)
		end
	end)
	
	wait(0.3)
	
	CreateSlashBurst()
	Tween(Bloom, 0.2, { Intensity = 1 })
	Tween(ColorCorrection, 0.1, { Brightness = 0.2 })
	
	Tween(LogoGlow3, 0.7, {
		Size = UDim2.new(0, BaseLogoSize * 1.5, 0, BaseLogoSize * 1.5),
		ImageTransparency = 0.75
	}, Enum.EasingStyle.Back)
	
	wait(0.05)
	
	Tween(LogoGlow2, 0.6, {
		Size = UDim2.new(0, BaseLogoSize * 1.3, 0, BaseLogoSize * 1.3),
		ImageTransparency = 0.55
	}, Enum.EasingStyle.Back)
	
	wait(0.05)
	
	Tween(LogoGlow, 0.5, {
		Size = UDim2.new(0, BaseLogoSize * 1.15, 0, BaseLogoSize * 1.15),
		ImageTransparency = 0.35
	}, Enum.EasingStyle.Back)
	
	wait(0.05)
	
	Tween(Logo, 0.5, {
		Size = UDim2.new(0, BaseLogoSize, 0, BaseLogoSize),
		ImageTransparency = 0
	}, Enum.EasingStyle.Back)
	
	Tween(ColorCorrection, 0.3, { Brightness = -0.08 })
	Tween(Bloom, 0.5, { Intensity = 0.5 })
	
	wait(0.4)
	
	CreateSlash(-45, 0, Config.AccentBlue, 2, 0.2)
	CreateSlash(45, 0.05, Config.LightBlue, 2, 0.2)
	
	Tween(TitleMain, 0.5, { TextTransparency = 0 })
	Tween(TitleStroke, 0.5, { Thickness = 2, Transparency = 0.4 })
	
	wait(0.1)
	
	Tween(TitleAccent, 0.5, { TextTransparency = 0 })
	Tween(AccentStroke, 0.5, { Thickness = 1.5, Transparency = 0.5 })
	
	wait(0.2)
	
	Tween(Subtitle, 0.4, { TextTransparency = 0.1 })
	
	wait(0.2)
	
	Tween(LoadingOuter, 0.3, { BackgroundTransparency = 0.5 })
	Tween(LoadingOuterStroke, 0.3, { Transparency = 0.3 })
	Tween(LoadingFill, 0.2, { BackgroundTransparency = 0 })
	Tween(LoadingPercent, 0.3, { TextTransparency = 0 })
	
	Connection = RunService.RenderStepped:Connect(function(delta)
		ElapsedTime = ElapsedTime + delta
		
		for _, arc in ipairs(Arcs) do
			arc.Gradient.Rotation = (arc.Gradient.Rotation + arc.Speed * arc.Direction * delta) % 360
		end
		
		local pulse = 1 + math.sin(ElapsedTime * 2.5) * 0.03
		OuterRing.Size = UDim2.new(0, 280 * pulse, 0, 280 * pulse)
		MiddleRing.Size = UDim2.new(0, 230 * pulse, 0, 230 * pulse)
		InnerRing.Size = UDim2.new(0, 180 * pulse, 0, 180 * pulse)
		OuterRingGlow.Size = UDim2.new(0, 320 * pulse, 0, 320 * pulse)
		
		local breathe = 1 + math.sin(ElapsedTime * 1.8) * 0.02
		local logoSize = BaseLogoSize * breathe
		Logo.Size = UDim2.new(0, logoSize, 0, logoSize)
		LogoGlow.Size = UDim2.new(0, logoSize * 1.15, 0, logoSize * 1.15)
		LogoGlow2.Size = UDim2.new(0, logoSize * 1.3, 0, logoSize * 1.3)
		LogoGlow3.Size = UDim2.new(0, logoSize * 1.5, 0, logoSize * 1.5)
		
		LogoGlow.ImageTransparency = 0.35 + math.sin(ElapsedTime * 3) * 0.1
		LogoGlow2.ImageTransparency = 0.55 + math.sin(ElapsedTime * 3 + 0.5) * 0.1
		LogoGlow3.ImageTransparency = 0.75 + math.sin(ElapsedTime * 3 + 1) * 0.08
		
		LoadingGradient.Offset = Vector2.new(math.sin(ElapsedTime * 4) * 0.2, 0)
	end)
	
	local loadingTime = 2
	local startTime = tick()
	
	while LoadingProgress < 1 do
		local elapsed = tick() - startTime
		LoadingProgress = math.min(elapsed / loadingTime, 1)
		local easedProgress = 1 - math.pow(1 - LoadingProgress, 3)
		
		LoadingFill.Size = UDim2.new(easedProgress, 0, 1, 0)
		LoadingPercent.Text = math.floor(easedProgress * 100) .. "%"
		wait()
	end
	
	wait(0.2)
	
	SlashesActive = false
	CreateSlashBurst()
	
	Subtitle.Text = Config.CompletedText
	Tween(Subtitle, 0.3, { TextColor3 = Config.White })
	Tween(LoadingPercent, 0.3, { TextColor3 = Config.ElectricBlue })
	
	Tween(Bloom, 0.1, { Intensity = 0.8 })
	Tween(ColorCorrection, 0.08, { Brightness = 0.15 })
	wait(0.1)
	Tween(Bloom, 0.25, { Intensity = 0.5 })
	Tween(ColorCorrection, 0.2, { Brightness = -0.08 })
	
	wait(0.4)
	
	if Connection then
		Connection:Disconnect()
	end
	
	Tween(Logo, 0.35, { ImageTransparency = 1 })
	Tween(LogoGlow, 0.35, { ImageTransparency = 1 })
	Tween(LogoGlow2, 0.35, { ImageTransparency = 1 })
	Tween(LogoGlow3, 0.35, { ImageTransparency = 1 })
	
	Tween(OuterRingStroke, 0.3, { Transparency = 1 })
	Tween(OuterRingGlowStroke, 0.3, { Transparency = 1 })
	Tween(MiddleRingStroke, 0.3, { Transparency = 1 })
	Tween(InnerRingStroke, 0.3, { Transparency = 1 })
	
	for _, arc in ipairs(Arcs) do
		Tween(arc.Stroke, 0.3, { Transparency = 1 })
	end
	
	Tween(TitleMain, 0.25, { TextTransparency = 1 })
	Tween(TitleAccent, 0.25, { TextTransparency = 1 })
	Tween(TitleStroke, 0.25, { Transparency = 1 })
	Tween(AccentStroke, 0.25, { Transparency = 1 })
	Tween(Subtitle, 0.25, { TextTransparency = 1 })
	Tween(LoadingOuter, 0.25, { BackgroundTransparency = 1 })
	Tween(LoadingOuterStroke, 0.25, { Transparency = 1 })
	Tween(LoadingFill, 0.25, { BackgroundTransparency = 1 })
	Tween(LoadingPercent, 0.25, { TextTransparency = 1 })
	
	Tween(Vignette, 0.35, { ImageTransparency = 1 })
	Tween(Background, 0.4, { BackgroundTransparency = 1 })
	Tween(Blur, 0.4, { Size = 0 })
	Tween(Bloom, 0.4, { Intensity = 0 })
	Tween(ColorCorrection, 0.4, {
		Brightness = 0,
		Contrast = 0,
		Saturation = 0,
	})
	
	wait(0.4)
	
	ScreenGui:Destroy()
	Blur:Destroy()
	ColorCorrection:Destroy()
	Bloom:Destroy()
end

spawn(StartAnimation)
